{
  "name": "Mokkify",
  "version": "1.1.0",
  "repository": "git@github.com:icevl/mokkify.git",
  "author": "Mikhail Kabanov <kabanov.mike@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "./node_modules/.bin/eslint ./src/ --ext .ts,.tsx,.js,.jsx --quiet --color",
    "type": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --write './src/**/*.{js,ts,json,tsx,scss}'",
    "start": "next start -p 7043",
    "cli": "node -r @swc-node/register ./src/app/database/cli.ts",
    "migrate": "node -r @swc-node/register ./src/app/database/migrate.ts",
    "migrate:create": "yarn migrate create --folder ./src/app/migrations --name"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/node": "20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "18.2.14",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "eslint": "8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "file-system-cache": "^2.4.4",
    "framer-motion": "^10.16.4",
    "is-uuid": "^1.0.2",
    "joi": "^17.11.0",
    "jose": "^5.1.0",
    "next": "14.0.1",
    "pg-hstore": "^2.3.4",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "18.2.0",
    "react-select": "^5.7.7",
    "react-use": "^17.4.0",
    "react18-json-view": "^0.2.6",
    "sequelize": "^6.34.0",
    "sqlite3": "^5.1.6",
    "typescript": "5.2.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.96",
    "@types/bcrypt": "^5.0.1",
    "@types/react-datepicker": "^4.19.1",
    "@types/umzug": "^2.3.5",
    "autoprefixer": "^10.4.16",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "sequelize-cli": "^6.6.2",
    "simple-pre-commit": "^1.2.1",
    "umzug": "^3.4.0"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "simple-pre-commit": "npx lint-staged",
  "lint-staged": {
    "*.{ts,js}": [
      "./node_modules/.bin/prettier -w './src/**/*.{ts,tsx}' ",
      "./node_modules/.bin/eslint ./src/ --ext .ts,.tsx,.js,.jsx --quiet --color --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
