{
  "name": "Mokkify",
  "version": "0.1.1",
  "private": true,
  "repository": "git@github.com:icevl/mokkify.git",
  "author": "Mikhail Kabanov <kabanov.mike@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "./node_modules/.bin/eslint ./src/ --ext .ts,.tsx,.js,.jsx --quiet --color",
    "type": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --write './src/**/*.{js,ts,json,tsx,scss}'",
    "start": "next start -p 7043",
    "cli": "node -r @swc-node/register ./src/app/database/cli.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/node": "20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "bcrypt": "^5.1.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.13",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "framer-motion": "^10.15.2",
    "is-uuid": "^1.0.2",
    "joi": "^17.9.2",
    "jose": "^4.14.4",
    "next": "13.4.13",
    "pg-hstore": "^2.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-view": "^1.21.3",
    "react-select": "^5.7.4",
    "react-use": "^17.4.0",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "typescript": "5.1.6",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@swc-node/register": "^1.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.66",
    "@types/bcrypt": "^5.0.0",
    "autoprefixer": "^10.4.15",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "sass": "^1.66.1",
    "sequelize-cli": "^6.6.1",
    "simple-pre-commit": "^1.2.1"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "simple-pre-commit": "npx lint-staged",
  "lint-staged": {
    "*.{ts,js}": [
      "./node_modules/.bin/prettier -w './src/**/*.{ts,tsx}' ",
      "./node_modules/.bin/eslint ./src/ --ext .ts,.tsx,.js,.jsx --quiet --color --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
