{
  "name": "Mokkify",
  "version": "1.0.0",
  "repository": "git@github.com:icevl/mokkify.git",
  "author": "Mikhail Kabanov <kabanov.mike@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "./node_modules/.bin/eslint ./src/ --ext .ts,.tsx,.js,.jsx --quiet --color",
    "type": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --write './src/**/*.{js,ts,json,tsx,scss}'",
    "start": "next start -p 7043",
    "cli": "node -r @swc-node/register ./src/app/database/cli.ts",
    "migrate": "node -r @swc-node/register ./src/app/database/migrate.ts",
    "migrate:create": "yarn migrate create --folder ./src/app/migrations --name"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "file-system-cache": "^2.4.4",
    "framer-motion": "^10.16.4",
    "is-uuid": "^1.0.2",
    "joi": "^17.10.1",
    "jose": "^4.14.6",
    "next": "13.4.19",
    "pg-hstore": "^2.3.4",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.17.0",
    "react-dom": "18.2.0",
    "react-json-editor-ajrm": "^2.5.14",
    "react-json-view": "^1.21.3",
    "react-select": "^5.7.4",
    "react-use": "^17.4.0",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "typescript": "5.2.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@swc-node/register": "^1.6.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.83",
    "@types/bcrypt": "^5.0.0",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-json-editor-ajrm": "^2.5.3",
    "@types/umzug": "^2.3.4",
    "autoprefixer": "^10.4.15",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "sequelize-cli": "^6.6.1",
    "simple-pre-commit": "^1.2.1",
    "umzug": "^3.3.1"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "simple-pre-commit": "npx lint-staged",
  "lint-staged": {
    "*.{ts,js}": [
      "./node_modules/.bin/prettier -w './src/**/*.{ts,tsx}' ",
      "./node_modules/.bin/eslint ./src/ --ext .ts,.tsx,.js,.jsx --quiet --color --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
